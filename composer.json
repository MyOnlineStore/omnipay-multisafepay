{
    "name": "myonlinestore/omnipay-multisafepay",
    "type": "library",
    "description": "MultiSafepay driver for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "multi safepay",
        "multisafepay",
        "omnipay",
        "pay",
        "payment"
    ],
    "homepage": "https://github.com/thephpleague/omnipay-multisafepay",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Macneil",
            "email": "adrian@adrianmacneil.com"
        },
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/thephpleague/omnipay-multisafepay/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "MyOnlineStore\\Omnipay\\MultiSafepay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyOnlineStore\\Omnipay\\MultiSafepay\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/MyOnlineStore/omnipay-tests"
        }
    ],
    "require": {
        "php": "^7.2",
        "omnipay/common": "^3",
        "ext-json": "*"
    },
    "require-dev": {
        "myonlinestore/coding-standard": "^2.0",
        "omnipay/tests": "dev-phpunit-8-9",
        "phpunit/phpunit": "^8.0",
        "vimeo/psalm": "^3.13"
    },
    "config": {
        "sort-packages": true
    }
}
